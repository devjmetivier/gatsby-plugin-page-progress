{
  "name": "gatsby-plugin-page-progress",
  "description": "Progress indicator for the top of pages as user scrolls",
  "version": "2.0.11",
  "author": "Devin Metivier <devinmetivier@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/dmetivier/gatsby-plugin-page-progress",
  "homepage": "https://github.com/dmetivier/gatsby-plugin-page-progress",
  "bugs": {
    "url": "https://github.com/dmetivier/gatsby-plugin-page-progress/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "plugin",
    "progress",
    "scroll",
    "indicator",
    "bar"
  ],
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . && uglifyjs gatsby-browser.js -cm -o gatsby-browser.js",
    "clean": "rm -rf node_modules",
    "watch": "babel -w src --out-dir .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@types/node": "^12.7.2",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-gatsby-package": "0.2.5",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-devjmetivier": "2.1.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3",
    "uglify-js": "^3.5.15"
  }
}
